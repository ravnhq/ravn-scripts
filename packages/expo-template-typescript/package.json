{
  "name": "@ravn-dev/expo-template-typescript",
  "version": "0.1.5",
  "description": "expo template for use with expo cli",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test:base": "cross-env TZ=UTC jest --config=config/jest/jest.config.js",
    "test": "jest --watch --coverage=false --changedSince=origin/master",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "format": "prettier '**/*.{js,ts,jsx,tsx}' --write",
    "validate": ""
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "expo": "^33.0.0",
    "i18n-js": "^3.3.0",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-web": "^0.11.4",
    "react-navigation": "^3.11.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@ravn-dev/eslint-config-ravn": "^0.1.5",
    "@ravn-dev/git-processes-config": "^0.0.7",
    "@ravn-dev/prettier-config": "^0.0.6",
    "@ravn-dev/typescript": "^0.0.7",
    "@testing-library/react-native": "^4.0.6",
    "@types/i18n-js": "^3.0.1",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.60",
    "@types/styled-components": "^4.1.16",
    "babel-jest": "^24.8.0",
    "babel-preset-expo": "^5.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "jest-expo": "^33.0.2",
    "jest-native": "^3.0.0",
    "jest-styled-components": "^6.3.3",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6",
    "typescript": "^3.4.5"
  },
  "gitHead": "08c92aa3bffac42b2b40b31550f308a1f09f44e4"
}
