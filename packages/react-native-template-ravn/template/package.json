{
  "name": "projectname",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "gql:codegen": "rm -rf src/graphql/api.tsx && graphql-codegen --config codegen.yml",
    "test": "cross-env TZ=UTC jest --config=config/jest/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:temp": "echo hook up testing",
    "tsc": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "format": "prettier 'src/**/*.{js,json,graphql}'",
    "format:write": "yarn format --list-different --write",
    "format:check": "yarn format --check",
    "validate": "npm-run-all --parallel -c tsc lint format:check -s \"test:temp --silent {1}\" --",
    "validate:ci": "yarn validate \"--maxWorkers=2\""
  },
  "dependencies": {
    "@apollo/react-components": "^3.1.3",
    "@apollo/react-hoc": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "@graphql-codegen/add": "^1.8.1",
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/introspection": "^1.8.1",
    "@graphql-codegen/typescript-operations": "^1.8.1",
    "@graphql-codegen/typescript-react-apollo": "^1.8.1",
    "@react-navigation/core": "^3.0.0",
    "apollo-boost": "^0.4.4",
    "apollo-client": "^2.6.4",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "i18n-js": "^3.3.0",
    "react": "16.8.1",
    "react-dom": "^16.10.2",
    "react-native": "0.61.2",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-localize": "^1.3.0",
    "react-native-reanimated": "^1.3.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-navigation": "^4.0.10",
    "react-navigation-hooks": "^1.1.0",
    "react-navigation-stack": "^1.9.4",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@graphql-codegen/typescript": "^1.8.1",
    "@ravn-dev/eslint-config-ravn": "^0.1.3",
    "@ravn-dev/git-processes-config": "^0.0.6",
    "@ravn-dev/prettier-config": "^0.0.6",
    "@ravn-dev/typescript": "^0.0.6",
    "@testing-library/jest-native": "^3.0.2",
    "@testing-library/react-native": "^4.0.14",
    "@types/i18n-js": "^3.0.1",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.5",
    "@types/react-native": "^0.60.19",
    "@types/styled-components": "^4.1.19",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^5",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.3",
    "metro-react-native-babel-preset": "^0.56.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.6.3"
  }
